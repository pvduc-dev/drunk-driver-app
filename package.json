{
  "name": "drunk-driver-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "customer:start:dev": "nest start customer --watch",
    "driver:start:dev": "nest start driver --watch",
    "driver:build": "nest build driver",
    "driver:start": "node dist/apps/driver/main",
    "admin:start:dev": "nest start admin --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/drunk-driver-app/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/drunk-driver-app/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.1.3",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "bullmq": "^5.53.0",
    "class-validator": "^0.14.2",
    "date-fns": "^4.1.0",
    "firebase-admin": "^13.4.0",
    "mongoose": "^8.14.2",
    "morgan": "^1.10.0",
    "nestjs-firebase": "^10.6.0",
    "nestjs-pino": "^4.4.0",
    "nestjs-twilio": "^4.4.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/customer-lib(|/.*)$": "<rootDir>/libs/customer-lib/src/$1",
      "^@app/db-lib(|/.*)$": "<rootDir>/libs/db-lib/src/$1",
      "^@app/geo-lib(|/.*)$": "<rootDir>/libs/geo-lib/src/$1",
      "^@app/notification(|/.*)$": "<rootDir>/libs/notification/src/$1",
      "^@app/notify-lib(|/.*)$": "<rootDir>/libs/notify-lib/src/$1",
      "^@app/otp-lib(|/.*)$": "<rootDir>/libs/otp-lib/src/$1",
      "^@lib/auth(|/.*)$": "<rootDir>/libs/auth/src/$1",
      "^@lib/auth-lib(|/.*)$": "<rootDir>/libs/auth-lib/src/$1",
      "^@lib/job-lib(|/.*)$": "<rootDir>/libs/job-lib/src/$1",
      "^@lib/sms-lib(|/.*)$": "<rootDir>/libs/sms-lib/src/$1"
    }
  }
}