{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/drunk-driver-app/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false,
    "plugins": [
      "@nestjs/swagger/plugin"
    ]
  },
  "monorepo": true,
  "projects": {
    "admin": {
      "type": "application",
      "root": "apps/admin",
      "entryFile": "main",
      "sourceRoot": "apps/admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "auth-lib": {
      "type": "library",
      "root": "libs/auth-lib",
      "entryFile": "index",
      "sourceRoot": "libs/auth-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth-lib/tsconfig.lib.json"
      }
    },
    "customer": {
      "type": "application",
      "root": "apps/customer",
      "entryFile": "main",
      "sourceRoot": "apps/customer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/customer/tsconfig.app.json"
      }
    },
    "db-lib": {
      "type": "library",
      "root": "libs/db-lib",
      "entryFile": "index",
      "sourceRoot": "libs/db-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/db-lib/tsconfig.lib.json"
      }
    },
    "driver": {
      "type": "application",
      "root": "apps/driver",
      "entryFile": "main",
      "sourceRoot": "apps/driver/src",
      "compilerOptions": {
        "tsConfigPath": "apps/driver/tsconfig.app.json"
      }
    },
    "notification": {
      "type": "library",
      "root": "libs/notification",
      "entryFile": "index",
      "sourceRoot": "libs/notification/src",
      "compilerOptions": {
        "tsConfigPath": "libs/notification/tsconfig.lib.json"
      }
    },
    "notify-lib": {
      "type": "library",
      "root": "libs/notify-lib",
      "entryFile": "index",
      "sourceRoot": "libs/notify-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/notify-lib/tsconfig.lib.json"
      }
    },
    "otp-lib": {
      "type": "library",
      "root": "libs/otp-lib",
      "entryFile": "index",
      "sourceRoot": "libs/otp-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/otp-lib/tsconfig.lib.json"
      }
    },
    "sms-lib": {
      "type": "library",
      "root": "libs/sms-lib",
      "entryFile": "index",
      "sourceRoot": "libs/sms-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/sms-lib/tsconfig.lib.json"
      }
    }
  }
}